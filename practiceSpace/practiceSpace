#include <iostream>

struct Package
{
    int zipCode{};
    char city[255]{};
    char state[255]{};
    int length{};
    int width{};
    int height{};
};

int main()
{
Package box[]{};

int length{(box / box[0]) - 1 };

std::cout << "Enter Zip Code: ";
std::cout >> cin >> box[0];

std::cout << "Enter City: ";
std::cout >> cin >> box[1];

std::cout << "Enter State: ";
std::cout >> cin >> box[2];

std::cout << "Enter Length: ";
std::cout >> cin >> box[3];

std::cout << "Enter Width: ";
std::cout >> cin >> box[4];

std::cout << "Enter Height: ";
std::cout >> cin >> box[5];

for (int i{0}; i < length; ++i}
{
    std::cout << box[i] << '\n';
}


    return 0;
}
