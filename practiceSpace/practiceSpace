#include <iostream>

void fourLoopsEnd()
{
    for(int i{1}; i <= 3; ++i)
    {
    std::cout << "We will print 3 times" << '\n';
    }
    std::cout << "We print after the loop ends within the function and will soon be destroyed after this function ends." << '\n';
}

int main()
{

fourLoopsEnd();

    return 0;
}    
