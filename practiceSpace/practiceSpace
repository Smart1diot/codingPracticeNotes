#include <iostream>

class Info
{
public: 
	int one{};
	int two{};
	int three{};

	void print()
	{
		std::cout << one << ' ' << two << ' ' << three << '\n';
	}

	int sum()
	{
		return one + two + three;
	}
};

int sumFunc(Info* container<int>& ds)
{
	return (ds.one + ds.two + ds.three);
}

int main()
{
	Info container{ 10, 20, 30 };

	container.print();

	std::cout << container.sum() << '\n';

	std::cout << sumFunc(container) << '\n';

	return 0;
}
