#include <iostream>
#include <string>

struct Container
{
    std::string text{};
    int num{};
    double dou{};
};

int main()
{
    Container box[]
    {
        "swag",
        35,
        44.88,
    };
    
    std::cout << "text: " << box[0] << '\n';
    std::cout << "num: " << box[1] << '\n';
    std::cout << "double: " << box[2] << '\n';

    return 0;
}
