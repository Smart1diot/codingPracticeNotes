#include <iostream>

struct Bool
{
    bool x{true}
};

int main()
{
Bool bool{};
    while(Bool::x)
    {
        std::out << "We will only print out one time then cease to exist...."
        Bool::x = false;
    }

std::cout << "Now that the reign of Bool is over, real int main()s rise up gamers!" << '\n';

return 0;
}
