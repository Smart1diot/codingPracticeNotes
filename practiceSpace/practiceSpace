#include <iostream>

struct Box
{
    int length{};
    int width{};
    int height{};
 };
 
 enum Location
 {
    seattle,
    spokane,
    snoqualmie,
 };

double determinePrice()
{
    switch()
    case seattle:       std::cout << 11.98;
    case spokane:       std::cout << 13.31;
    case snoqualmie:    std::cout << 12.28; 
}

int main()
{
Box firstBox{11, 11, 11};

return 0;
}
