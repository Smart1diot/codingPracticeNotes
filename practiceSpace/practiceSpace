#include <iostream>
#include <string>
#include <string_view>

class SwagYolo
{
private:
	std::string foo{};

public:
	SwagYolo()
		: foo{ "hello world" } {
		std::cout << "setting up hello world..." << '\n';
	}

	void print()
	{
		std::cout << foo << '\n';
	}

};

int main()
{
	SwagYolo foo{};

	foo.print();

	return 0;
}
