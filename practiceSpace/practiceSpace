2/13/2023 notes on storing and accessing multiple data forms 
#include <iostream>
#include <string>


struct Balls
{
	std::string text{};
	int age{};
	double cash{};
};

int main()
{
	Balls text{ "swag", 18, 44.88}; //So far best form of storing multiple data forms

	Balls text1[]{ "swag",18,44.99 }; //array that stores multiples data forms but accesing text isnt that easys

	std::string text2[]{ "swag", "cum", "bootyhole" }; //So far best form of storing strings

	std::cout << text.text << ' ' << text.age << ' ' << text.cash << '\n';
	//std::cout << text1[0] << ' ' << text1[1] << ' ' << text1[2] << '\n';
	std::cout << text2[0] << ' ' << text2[1] << ' ' << text2[2] << '\n';

	return 0;
}
