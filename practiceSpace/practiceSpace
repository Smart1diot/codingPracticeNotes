#include <iostream>

struct Container
{
    int length{};
    int width{};
    int height{};
    int weight{};
};

Container backPack[4]{};

int main()
{
    backPack[0].length = 21;
    backPack[1].width = 15;
    backPack[2].height = 11;
    backPack[3].weight = 21;

    std::cout << "Dimentional weight of our package is: " << (backPack[0].length * backPack[1].width * backPack[2].height) / 166 << '\n';
    std::cout << "Actual weight of our package is: " << backPack[3].weight << '\n';

    return 0;
}
