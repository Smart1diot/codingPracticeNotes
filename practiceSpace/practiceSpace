// 2/14/2023

#include <iostream>
#include <string>

struct Balls
{
	std::string text{};
	int age{};
	double cash{};
};


int main()
{
	Balls text1[]
	{
		{"swag", 25, 44.88 },
		{"dog", 35, 88.99},
		{"cat", 45, 22.22},
	};

	int textItems{ sizeof(text1) / sizeof(text1[0]) };

	for (int i{ 0 }; i < textItems - 1; ++i)
	{
		for (int j{ i + 1 }; j < textItems - 1; ++j)
		{
			if (text1[j] == "dog")
			{
				std::cout << "dog";
			}
		}
	}

	
	
	return 0;
}
